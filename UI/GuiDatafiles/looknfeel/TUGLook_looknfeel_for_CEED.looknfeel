                <Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="3" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="3" /></Dim>
                    <Dim type="Width" ><ImageDim name="TUGLook/HeaderBarSortUp" dimension="Width" /></Dim>
                    <Dim type="Height" ><ImageDim name="TUGLook/HeaderBarSortUp" dimension="Height" /></Dim>
                </Area>
                <Image name="TUGLook/HeaderBarSortDown" />
