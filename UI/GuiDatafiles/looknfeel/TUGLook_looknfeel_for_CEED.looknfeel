    <!--
    ***************************************************
        TUGLook/StaticShared
        (Shared imagery components for static widgets)
    ***************************************************
    -->

    <WidgetLook name="TUGLook/StaticShared">
        <ImagerySection name="frame">
            <FrameComponent>
                <Image component="TopLeftCorner" name="TUGLook/StaticTopLeft" />
                <Image component="TopRightCorner" name="TUGLook/StaticTopRight" />
                <Image component="BottomLeftCorner" name="TUGLook/StaticBottomLeft" />
                <Image component="BottomRightCorner" name="TUGLook/StaticBottomRight" />
                <Image component="LeftEdge" name="TUGLook/StaticLeft" />
                <Image component="RightEdge" name="TUGLook/StaticRight" />
                <Image component="TopEdge" name="TUGLook/StaticTop" />
                <Image component="BottomEdge" name="TUGLook/StaticBottom" />
                <ColourRectProperty name="FrameColours" />
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="background">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge" ><ImageDim name="TUGLook/StaticLeft" dimension="Width" /></Dim>
                    <Dim type="TopEdge" ><ImageDim name="TUGLook/StaticTop" dimension="Height" /></Dim>
                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="RightEdge" />
                            <ImageDim name="TUGLook/StaticRight" dimension="Width" />
                        </OperatorDim>
                    </Dim>
                    <Dim type="BottomEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="BottomEdge" />
                            <ImageDim name="TUGLook/StaticBottom" dimension="Height" />
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="TUGLook/StaticBackdrop" />
                <!--<ColourRectProperty name="BackgroundColours" />-->
                <Colours topLeft="FFFFFFFF" topRight="FFFFFFFF" bottomLeft="FFFFFFFF" bottomRight="FFFFFFFF" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>
        <ImagerySection name="background_noframe">
            <ImageryComponent>
                <Image name="TUGLook/StaticBackdrop" />
                <!--<ColourRectProperty name="BackgroundColours" />-->
                <Colours topLeft="FFFFFFFF" topRight="FFFFFFFF" bottomLeft="FFFFFFFF" bottomRight="FFFFFFFF" />
                <VertFormat type="Stretched" />
                <HorzFormat type="Stretched" />
            </ImageryComponent>
        </ImagerySection>
    </WidgetLook>

